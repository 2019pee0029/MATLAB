f1_2D=makeGauss2D(x,0,p.sig1);
f2_2D=makeGauss2D(x,1,p.sig2);
int1=500;
i500=zeros(10,11,3);
o=0;i500(:,:,1)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));
o=100;i500(:,:,2)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));
o=1000;i500(:,:,3)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));
for ii=1:3
    dipshow(i500(:,:,ii));
    name = twoSources_Sep1

int1=1000;
i1000=zeros(10,11,3);
o=0;i1000(:,:,1)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));
o=100;i1000(:,:,2)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));
o=1000;i1000(:,:,3)=double(noise(resample(int1*f1_2D+int1*f2_2D+o,.2), 'poisson'));